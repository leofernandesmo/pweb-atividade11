<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <!-- registeredLogins.ejs -->
<h1>Registered Logins</h1>
<table>
    <tr>
        <th>Name</th>
        <th>Username</th>
        <th>Password</th>
    </tr>
    <% users.forEach(user => { %>
        <tr>
        <td><%= user.name %></td>
        <td><%= user.username %></td>
        <td><%= user.password %></td>
        <td><a href="/delete/<%= user.username %>">Delete</a></td>
        </tr>
    <% }); %>
</table>
    
</body>
</html>